call janus#disable_plugin('supertab', 'using youcompleteme instead')

"disable narrow region, slows down open/close of NERDTree
call janus#disable_plugin('Nrrwrgn', 'not used')
call janus#disable_plugin('snipmate', 'using UltiSnips instead')

set guifont=monaco:h12

"disable backup files, avoids getting warnings after vim closed without save"
set nobackup
